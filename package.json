{
  "name": "venvan-web",
  "version": "0.0.0",
  "engines": {
    "node": ">=10.15.1"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run build && node ./build/main.js",
    "start:prod": "node ./build/main.js",
    "e2e": "ng e2e",
    "dev:server": "nodemon",
    "dev:client": "ng serve --disable-host-check --open --proxy-config proxy.conf.json --aot",
    "build": "npm-run-all -p build:*",
    "heroku-postbuild": "npm-run-all -p build:*",
    "build:server": "tsc -p src/server/tsconfig.build.json",
    "build:client": "ng build --prod --aot",
    "test": "npm-run-all -p test:*",
    "test:client": "jest --config src/client/jest.config.js",
    "test:server": "jest --config src/server/jest.config.js",
    "test:server:e2e": "jest --config src/server/test/jest-e2e.json",
    "test:server:cov": "jest --config src/server/jest.config.js --coverage",
    "test:client:cov": "jest --config src/client/jest.config.js --coverage",
    "format": "npm-run-all -p format:*",
    "format:client": "prettier --write \"src/client/*.ts\"",
    "format:server": "prettier --write \"src/server/*.ts\"",
    "debug:server:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "postinstall": "npm run prestart:prod && ivy-ngcc",
    "prod:server": "node dist/main.js",
    "lint": "npm-run-all -p lint:*",
    "lint:server": "tslint -p src/server/tsconfig.json",
    "lint:client": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.0",
    "@angular/cdk": "^8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/material": "^8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "@briebug/jest-schematic": "^1.6.0",
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "core-js": "^2.5.4",
    "express": "^4.16.4",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.1",
    "@angular/cli": "~8.0.1",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@nestjs/testing": "^5.1.0",
    "@types/express": "^4.16.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/supertest": "^2.0.5",
    "angular2-template-loader": "^0.6.2",
    "codelyzer": "~4.5.0",
    "husky": "^1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "24.1.0",
    "jest-preset-angular": "6.0.2",
    "jest-sonar-reporter": "^2.0.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.16.4",
    "protractor": "~5.4.0",
    "supertest": "^3.1.0",
    "ts-jest": "^24.0.0",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5",
    "wallaby-webpack": "^3.9.14"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test",
      "pre-push": "yarn lint && yarn test",
      "commit-msg": "commitlint -e"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
